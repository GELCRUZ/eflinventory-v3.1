{
    "name": "chrisidakwo/efl-inventory",
    "description": "A small-store inventory management and point-of-sale system",
    "keywords": ["store", "inventory", "pos", "sale", "product", "barcode", "scanner", "stock", "batch", "fifo"],
    "license": "MIT",
    "type": "product",
    "require": {
        "php": ">=7.0.0",
        "ext-intl": "*",
        "ext-json": "*",
        "arcanedev/log-viewer": "^5.0",
        "darryldecode/cart": "^4.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^2.0",
        "mike42/escpos-php": "^1.6",
        "nao-pon/flysystem-google-drive": "~1.1",
        "phpoffice/phpspreadsheet": "^1.0",
        "rah/danpu": "^2.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "doctrine/dbal": "^2.10",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
